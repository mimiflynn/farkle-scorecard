{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "eol-last": "error",
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "max-len": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "no-unneeded-ternary": 0,
    "no-useless-escape": 0,
    "prefer-template": 0,
    "import/prefer-default-export": 0,
    "quote-props": 0,
    "react/forbid-prop-types": 0,
    "react/no-multi-comp": 0,
    "react/no-unused-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0,
    "space-before-function-paren": [2, {"anonymous": "always", "named": "always"}]
  }
}


